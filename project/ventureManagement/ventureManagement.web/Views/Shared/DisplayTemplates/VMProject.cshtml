@model VentureManagement.Models.VMProject

@{
    var title = ViewData.ContainsKey("title") ? ViewData["title"].ToString() : "";
    var itemId = ViewData.ContainsKey("itemId") ? ViewData["itemId"].ToString() : "";
    var visibale = ViewData.ContainsKey("visibale") ? ViewData["visibale"].ToString() : "";
    var url = ViewData.ContainsKey("url") ? ViewData["url"].ToString() : "";
}

@(
 Html.X().FormPanel()
        .ItemID(itemId)
        .Title(title)
        .Url(url)
        .DefaultAnchor("100%")
        .StyleSpec("margin-bottom:5px;")
        .Frame(true)
        .Items(
            Html.X().HiddenFor(p=>p.ProjectId),
            Html.X().TextFieldFor(p=>p.ProjectName),
            Html.X().TextFieldFor(p=>p.ProjectLocation)
        )
      )

@Html.DisplayFor(t=>t.Organization, new {itemId = "OrganizationId", title = "施工单位信息",visibale = true})
@Html.DisplayFor(t=>t.User, new {itemId = "ThreatCaseOwnerId", title = "施工负责人",visibale = true})
